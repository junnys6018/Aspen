/*then branch 1
then branch 3
else branch 4
else if branch 5:1
*/
if (true) {
    print "then branch 1";
}

if (false) {
    print "then branch 2";
}

if (true) {
    print "then branch 3";
} else {
    print "else branch 3";
}

if (false) {
    print "then branch 4";
} else {
    print "else branch 4";
}

if (false) {
    print "then branch 5";
} else if (true) {
    print "else if branch 5:1";
} else if (true) {
    print "else if branch 5:2";
} else {
    print "else branch 5";
}
