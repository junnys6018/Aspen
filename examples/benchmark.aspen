fn fib(n i64) i64 {
    if (n <= 1) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

for (let i i64 = 0; i <= 30; i = i + 1) {
    let start i64 = clock();

    let n i64 = fib(i);

    let msElapsed i64 = (clock() - start) / 1000;

    print "calculated fib(" + itoa(i) + ") = " + itoa(n) + " in " + itoa(msElapsed) + "ms";
}
